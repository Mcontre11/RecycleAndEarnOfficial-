<%- include("partials/header") %>
<link rel="stylesheet" href="board.css">

<div class="container"> </div>


  <div class="leaderBoard"> </div>

    <!-- LOCAL INFORMATION -->
    <div class="col-sm-6"> 
      <div class="board">
        <h3><span class="fa fa-star"></span> Leader Board<span class="fa fa-star"></span> 
        </h3>
   
        <ul class="messages">
          <% 
          const barCodes = Object.keys(barCodeCount)
          for(var i=0; i<barCodes.length; i++) {

            %>
          
            <li class="counter">
              <p class="count">
              <span><%= barCodes[i] %></span>
            </p>
            <p class="the">
              <span><%= barCodeCount[barCodes[i]] %></span>
            </p>
            </li>
          <% } %>
         
          </ul>
      </div>
     
      
     </div

     
       
          

   
           


 